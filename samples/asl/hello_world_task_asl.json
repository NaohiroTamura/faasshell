{
    "name": "hello_world_task_asl.json",
    "input" : {
        "name": "FaaS Shell"
    },
    "asl" :{
        "Comment": "A Hello World example of the Amazon States Language using an AWS Lambda function",
        "StartAt": "HelloWorld",
        "States": {
            "HelloWorld": {
                "Type": "Task",
                "Resource": "hello",
                "TimeoutSeconds": 300,
                "HeartbeatSeconds": 60,
                "End": true
            }
        }
    }
}
